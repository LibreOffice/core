ImageList IL_SECTION_BITMAPS
{
    Prefix = "re";
    MaskColor = IMAGE_MASK_COLOR ;
    IdList =
    {
        BMP_HIDE           /*1*/ ;
        BMP_NO_HIDE        /*2*/ ;
        BMP_PROT_HIDE      /*3*/ ;
        BMP_PROT_NO_HIDE   /*4*/ ;
    };
    IdCount = { 4 ; };
};

