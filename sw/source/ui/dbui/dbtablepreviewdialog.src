    };
    Window WIN_BEAMER
    {
        Pos = MAP_APPFONT ( 6 , 14 ) ;
        Size = MAP_APPFONT ( 338 , 150 ) ;
    };
    OKButton        PB_OK
    {
        Pos = MAP_APPFONT ( 150 , 180 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
        DefButton = TRUE ;
        Text [ de ] = "Schließen";
        Text [ en-US ] = "~Close";
    };
};

