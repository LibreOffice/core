    {
        Border = TRUE ;
        Size = MAP_APPFONT ( 45 , 11 ) ;
        ReadOnly = TRUE ;
        SVLook = TRUE ;
        Center = TRUE ;
    };
    Edit ED_FORMULA
    {
        Border = TRUE ;
        TabStop = TRUE ;
        Size = MAP_APPFONT ( 500 , 11 ) ;
        SVLook = TRUE ;
    };
};
