    HelpID = HID_CALC_TOOLBOX ;
    SVLook = TRUE ;
    Border = TRUE ;
    ItemList =
    {
        ToolBoxItem
        {
            Identifier = FN_FORMULA_CALC ;
            HelpId = HID_TBX_FORMULA_CALC ;
            DropDown = TRUE ;
            Text [ en-US ] = "Formula" ;
        };
        ToolBoxItem
        {
            Identifier = FN_FORMULA_CANCEL ;
            HelpId = HID_TBX_FORMULA_CANCEL ;
            Text [ en-US ] = "Cancel" ;
        };
        ToolBoxItem
        {
            Identifier = FN_FORMULA_APPLY ;
            HelpId = HID_TBX_FORMULA_APPLY ;
            Text [ en-US ] = "Apply" ;
        };
    };
