  <object class="GtkListStore" id="liststore">
    <columns>
      <!-- column-name gchararray1 -->
      <column type="gchararray"/>
      <!-- column-name gint1 -->
      <column type="gint"/>
    </columns>
    <data>
      <row>
        <col id="0" translatable="yes">Top</col>
        <col id="1">0</col>
      </row>
      <row>
        <col id="0" translatable="yes">Centered</col>
        <col id="1">1</col>
      </row>
      <row>
        <col id="0" translatable="yes">Bottom</col>
        <col id="1">2</col>
      </row>
    </data>
  </object>
