        </rng:group>
        <rng:group>
          <rng:attribute name="office:value-type">
            <rng:value>boolean</rng:value>
          </rng:attribute>
          <rng:optional>
            <rng:attribute name="calcext:value-type">
              <rng:value>boolean</rng:value>
            </rng:attribute>
          </rng:optional>
          <rng:attribute name="office:boolean-value">
