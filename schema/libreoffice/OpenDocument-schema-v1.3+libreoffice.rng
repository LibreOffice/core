        <rng:group>
          <rng:attribute name="office:value-type">
            <rng:value>string</rng:value>
          </rng:attribute>
          <rng:optional>
            <!-- OFFICE-3759 -->
            <rng:attribute name="calcext:value-type">
              <rng:choice>
                <rng:value>string</rng:value>
                <rng:value>error</rng:value>
