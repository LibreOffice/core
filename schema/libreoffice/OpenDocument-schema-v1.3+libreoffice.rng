        <rng:ref name="draw-caption-attlist"/>
        <rng:ref name="common-draw-position-attlist"/>
        <rng:ref name="common-draw-size-attlist"/>
        <rng:ref name="common-draw-shape-with-text-and-styles-attlist"/>
        <rng:optional>
