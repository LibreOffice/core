<define name="form-property-type-and-value-list">
    <choice>
        <group>
            <attribute name="office:value-type">
                <value>float</value>
            </attribute>
            <zeroOrMore>
                <element name="form:list-value">
                    <attribute name="office:value">
                        <ref name="double"/>
                    </attribute>
                </element>
            </zeroOrMore>
        </group>
        <group>
            <attribute name="office:value-type">
                <value>percentage</value>
            </attribute>
            <zeroOrMore>
                <element name="form:list-value">
                    <attribute name="office:value">
