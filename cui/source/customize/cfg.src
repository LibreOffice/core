        WordBreak = TRUE;
    };
    FixedLine FL_SEPARATOR
    {
        Pos = MAP_APPFONT ( 0, 120 );
        Size = MAP_APPFONT ( 240, 8 );
    };
    Color IMAGE_BTN_COLOR
    {
        Red = 0xC000 ;
        Green = 0xC000 ;
        Blue = 0xC000 ;
    };
};
