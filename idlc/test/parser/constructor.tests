    f();
};


EXPECT FAILURE "constructor.tests 4":
interface X {};
service S: X {
    f();
    f();
};


EXPECT FAILURE "constructor.tests 5":
interface X {
    void f([in] any... p);
};


EXPECT FAILURE "constructor.tests 6":
