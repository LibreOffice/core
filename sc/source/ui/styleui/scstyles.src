#define IMPL_FAMILY(family,bmp,filter) \
    StyleFamily = family;                       \
    StyleImage = Image                          \
    {                                           \
       ImageBitmap = Bitmap { File = bmp; };    \
       MaskColor   = STD_MASKCOLOR;             \
    };                                          \
    FilterList = { filter }

