TabDialog RID_SCDLG_HFED_HEADER
{
    OutputSize = TRUE ;
    SVLook = TRUE ;
    Size = MAP_APPFONT ( 270 , 140 ) ;
    Text [ en-US ] = "Headers" ;
    Moveable = TRUE ;
     // Closeable = TRUE;
    TabControl 1
    {
        OutputSize = TRUE ;
        SVLook = TRUE ;
        Pos = MAP_APPFONT ( 6 , 3 ) ;
        Size = MAP_APPFONT ( 260 , 135 ) ;
        PageList =
        {
            PageItem
            {
                Identifier = 1 ;
                HEADER_RIGHT
                PageResID = RID_SCPAGE_HFED_HR ;
            };
            PageItem
            {
                Identifier = 2 ;
                HEADER_LEFT
                PageResID = RID_SCPAGE_HFED_HL ;
            };
        };
    };
};

