        Text [ en-US ] = "Select the language to use for import" ;
    };

    RadioButton RB_AUTOMATIC
    {
        HelpID = "sc:RadioButton:RID_SCDLG_TEXT_IMPORT_OPTIONS:RB_AUTOMATIC";
        Pos = MAP_APPFONT( 12, 20 ) ;
