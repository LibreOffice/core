    HelpId = HID_SCDLG_CONFLICTS ;
    SVLook = TRUE ;
    Size = MAP_APPFONT ( 338 , 192 ) ;
    Text [ en-US ] = "Resolve Conflicts" ;
    Moveable = TRUE ;
    Sizeable = TRUE ;
    Closeable = TRUE ;
    FixedText FT_CONFLICTS
