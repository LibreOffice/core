        Size = MAP_APPFONT ( 50 , 14 ) ;
    };
    String STR_TITLE_CONFLICT
    {
        Text [ en-US ] = "Conflict" ;
    };
    String STR_TITLE_AUTHOR
    {
        Text [ en-US ] = "Author" ;
    };
    String STR_TITLE_DATE
