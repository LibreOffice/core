FloatingWindow RID_NAVIGATOR
{
    OutputSize = TRUE ;
    SVLook = TRUE ;
    Size = MAP_APPFONT ( 200 , 250 ) ;
    Text [ en-US ] = "Report navigator" ;
    HelpId = HID_RPT_NAVIGATOR_DLG;
    Moveable = TRUE ;
    Closeable = TRUE ;
    Sizeable = TRUE;

    Control 1
    {
        Pos = MAP_APPFONT( 0, 0 );
        Size = MAP_APPFONT( 200, 250 );
        Border = TRUE;
        TabStop = TRUE;
    };
};
#define NAVIGATOR_IMAGEIDS \
