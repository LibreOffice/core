
set platform = `uname`

cd dmake

if ($platform == "SunOS") then
   make Solaris2.1
else if ($platform == "Linux") then
   make linux
else if ($platform == "NetBSD") then
   make linux
else if ($platform == "FreeBSD") then
   make linux
else if ($platform == "OSF1") then
   make tru64
else if ($platform == "Irix" || $platform == "Irix64") then
   make irix
else if ($platform == "Darwin") then
   make macosx
endif

if "$?" != "0" exit

cd ..

