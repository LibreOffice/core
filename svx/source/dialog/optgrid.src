    {
        HelpID = "svx:MetricField:RID_SVXPAGE_GRID:MTR_FLD_ANGLE";
        Border = TRUE ;
        Pos = MAP_APPFONT ( 194 , 137  ) ;
         // 190, 111
        Size = MAP_APPFONT ( 54 , 12 ) ;
        Hide = TRUE;
        TabStop = TRUE ;
        Repeat = TRUE ;
        Spin = TRUE ;
