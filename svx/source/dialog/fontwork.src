    {
        HelpID = "svx:ListBox:RID_SVXDLG_FONTWORK:CLB_SHADOW_COLOR";
        Border = TRUE ;
        Pos = MAP_APPFONT ( 6 , 174 ) ;
        Size = MAP_APPFONT ( 71 , 60 ) ;
        DropDown = TRUE ;
        QuickHelpText [ en-US ] = "Shadow Color" ;
        Text = "-";
    };

#define FONTWORK_IDLIST \
        IdList =            \
        {                   \
            TBI_STYLE_OFF ; \
            TBI_STYLE_ROTATE ;  \
            TBI_STYLE_UPRIGHT ; \
            TBI_STYLE_SLANTX ;  \
            TBI_STYLE_SLANTY ;  \
