            TBI_ADJUST_LEFT ;   \
            TBI_ADJUST_CENTER ; \
            TBI_ADJUST_RIGHT ;  \
            TBI_ADJUST_AUTOSIZE ; \
            TBI_SHOWFORM ;  \
            TBI_OUTLINE ;   \
            TBI_SHADOW_OFF ;    \
            TBI_SHADOW_NORMAL ; \
            TBI_SHADOW_SLANT ; \
            TBI_SHADOW_XDIST ; \
            TBI_SHADOW_YDIST ; \
            TBI_SHADOW_ANGLE ; \
            TBI_SHADOW_SIZE ; \
            TBI_DISTANCE ; \
            TBI_TEXTSTART ; \
        };                 \
        IdCount = { 21 ; };

    #define IMAGE_STDBTN_COLOR Color { Red = 0xff00; Green = 0x0000; Blue = 0xff00; }

    ImageList IL_FONTWORK
    {
        Prefix = "fw";
        MaskColor = IMAGE_STDBTN_COLOR ;
        FONTWORK_IDLIST
    };
