        Text [ de ] = "%PRODUCTNAME Schnellstart" ;
        Text [ en-US ] = "%PRODUCTNAME Quickstarter";
    };
    CheckBox CB_QUICKLAUNCH
    {
        Pos = MAP_APPFONT( COL1, ROW9 );
