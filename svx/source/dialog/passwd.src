    };
    FixedText FT_NEW_PASSWD
    {
        Pos = MAP_APPFONT ( 12 , 45 ) ;
        Size = MAP_APPFONT ( 57 , 8 ) ;
        Text [ en-US ] = "Pa~ssword" ;
