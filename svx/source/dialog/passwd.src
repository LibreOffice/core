    {
        HelpID = "svx:Edit:RID_SVXDLG_PASSWORD:ED_NEW_PASSWD";
        Border = TRUE ;
        Pos = MAP_APPFONT ( 71 , 43 ) ;
        Size = MAP_APPFONT ( 71 , 12 ) ;
        PassWord = TRUE ;
    };
    FixedText FT_REPEAT_PASSWD
