        WordBreak = TRUE ;
    };
    String ABOUT_STR_DEVELOPER
    {
        Text = "" ;
    };
    StringArray ABOUT_STR_DEVELOPER_ARY
    {
        OOO_CONTRIBUTOR_LIST
    };
    String ABOUT_STR_COPYRIGHT_ADABAS
    {
        Text =  "" ;
    };
    String ABOUT_STR_ACCEL
    {
        Text = "SDT" ;
    };
};

