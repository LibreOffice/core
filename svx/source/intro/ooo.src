ModalDialog RID_DEFAULTABOUT
{
    OutputSize = TRUE ;
    Size = MAP_APPFONT ( 245 , 200 ) ;
    Moveable = TRUE ;
    SVLook = TRUE ;
    TEXT_DEFAULTABOUT
    OKButton ABOUT_BTN_OK
    {
        DefButton = TRUE ;
        Pos = MAP_APPFONT ( 174 , 6 ) ;
        Size = MAP_APPFONT ( 50 , 14 ) ;
    };
    FixedText ABOUT_FTXT_VERSION
    {
        Pos = MAP_APPFONT ( 54 , 6 ) ;
        Size = MAP_APPFONT ( 118 , 16 ) ;
        WordBreak = TRUE ;
        Text =  "$(VER)";
       };
    FixedText ABOUT_FTXT_COPYRIGHT
    {
        Pos = MAP_APPFONT ( 54 , 25 ) ;
        Size = MAP_APPFONT ( 168 , 51 ) ;
         Text [ de ] =  "Copyright 2000-2004 Sun Microsystems Inc.\nThis product has been created by " OOO_VENDOR " based on OpenOffice.org using the " OOO_LICENSE ".\nOpenOffice.org acknowledges all community members, especially those mentioned at http://www.openoffice.org/welcome/credits.html.";
