[Desktop Entry]
Version=1.0
Terminal=false
Type=Application
Categories=Office;
Exec=${UNIXBASISROOTNAME} --quickstart --nologo --nodefault
NoDisplay=true
Name=%PRODUCTNAME Quickstarter
Comment=Hook for quickstarter startup
X-GIO-NoFuse=true
X-KDE-Protocols=file,http,ftp,webdav
