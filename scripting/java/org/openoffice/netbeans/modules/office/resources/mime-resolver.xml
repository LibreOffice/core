<MIME-resolver>

    <!-- Resolve according to root element: -->
    <file>
        <ext name="xml"/>
        <resolver mime="text/x-parcel+xml">
            <xml-rule>
                <element name="parcel"/>
            </xml-rule>
        </resolver>
    </file>

</MIME-resolver>
