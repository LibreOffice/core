          <info>
            <desc>Specifies, whether CJK font settings are available (true) or
            not (false).</desc>
          </info>
          <value>false</value>
        </prop>
        <prop oor:name="VerticalText" oor:type="xs:boolean" oor:nillable="false">
          <!-- UIHints: setup -->
          <info>
            <desc>Specifies, whether vertical text is available (true) or not
