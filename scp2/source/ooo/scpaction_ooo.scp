#ifdef MACOSX
ScpAction scp_Copy_Thirdpartylicense_Readme_Html
    Copy = "THIRDPARTYLICENSEREADME.html";
    Name= "THIRDPARTYLICENSEREADME.html";
    Subdir = "LICENSEs";
End
