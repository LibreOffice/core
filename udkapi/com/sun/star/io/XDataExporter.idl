             [in] com::sun::star::lang::XComponent Component,
             [in] com::sun::star::io::XDataTransferEventListener aListener );

    //-------------------------------------------------------------------------

    // DocMerge from xml: method com::sun::star::io::XDataExporter::cancel
    /** cancels the export process.
     */
    [oneway] void cancel();

