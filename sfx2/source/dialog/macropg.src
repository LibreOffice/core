    String STR_EVENT
    {
        Text [ de ] = "Ereignis" ;
        Text [ en-US ] = "Event" ;
    };
    String STR_ASSMACRO
    {
        Text [ de ] = "Zugewiesenes Makro";
        Text [ en-US ] = "Assigned macro";
    };
    Control LB_EVENT
    {
        Border = TRUE ;
        Pos = MAP_APPFONT ( COL0 , ROW0 ) ;
