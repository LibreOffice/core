    FixedText FT_USER_LAST
    {
        Pos = MAP_APPFONT(MARGINLEFT, MARGINTOP+ROWHEIGHT*9+FTADD);
        Size = MAP_APPFONT(USERINDENT, ROWHEIGHT);
        Text [ en-US ] = "~Last name";
    };
    Edit ED_USER_LAST
    {
        Border = TRUE;
