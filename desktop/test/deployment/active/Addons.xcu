      <node o:name="org.openoffice.test.desktop.deployment.active"
          o:op="replace">
        <prop o:name="Title" xml:lang="en-US">
          <value>active</value>
        </prop>
        <node o:name="Submenu">
          <node o:name="1" o:op="replace">
