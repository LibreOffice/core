+                 return !stricmp(aRealValue->Value.lpszA,aFilterValue->Value.lpszA);
+                 break;
+             case FL_PREFIX :
+                 return stristr(aRealValue->Value.lpszA,aFilterValue->Value.lpszA) == aRealValue->Value.lpszA;
+                 break;
+             case FL_SUBSTRING :
