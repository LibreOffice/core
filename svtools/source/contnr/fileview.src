Menu RID_FILEVIEW_CONTEXTMENU
{
    ItemList =
    {
        MenuItem
        {
            Identifier = MID_FILEVIEW_DELETE ;
            HelpId = HID_FILEVIEW_MENU_DELETE ;
            Text [ en-US ] = "~Delete";
        };
        MenuItem
        {
            Identifier = MID_FILEVIEW_RENAME ;
