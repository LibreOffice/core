TabPage RID_PAGE_FINAL
{
    HelpID = "extensions:TabPage:RID_PAGE_FINAL";
    SVLook  = TRUE ;
    Size    = MAP_APPFONT ( WINDOW_SIZE_X , WINDOW_SIZE_Y ) ;

    FixedText FT_FINISH_EXPL
