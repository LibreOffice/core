        SVLook = TRUE;
        Border = TRUE;
        DropDown = TRUE ;
        AutoHScroll = TRUE ;
    };

    PushButton PB_BROWSE
    {
        HelpID = "extensions:PushButton:RID_PAGE_FINAL:PB_BROWSE";
