TabPage RID_PAGE_LCW_CONTENTSELECTION_TABLE
{
    HelpID = "extensions:TabPage:RID_PAGE_LCW_CONTENTSELECTION_TABLE";
    SVLook = TRUE ;
    Size = MAP_APPFONT ( WINDOW_SIZE_X , WINDOW_SIZE_Y ) ;

    Text [ en-US ] = "Table Selection";

    FixedLine FL_FRAME
    {
        Pos = MAP_APPFONT ( 4 , 40  ) ;
