[project]
	description = LibreOffice main, development code repository
	state = active
[receive]
	requireChangeId = true
[submit]
	action = cherry pick
	mergeContent = true
[access]
	inheritFrom = Dev-ACL-Template
[access "refs/*"]
	read = group ReplicateFDO
[access "refs/heads/aoo/*"]
	pushMerge = group SvnGit
