WarningBox WARN_SAVEFORQUERIES
{
    Message [ de ] = "Um die $objects$ einer Datenquelle zu administrieren, muss diese gespeichert werden.\n"
        "Sollen sämtliche bis jetzt in diesem Dialog vorgenommene Änderungen jetzt gespeichert werden?";
    Message [ en-US ] = "To administrate the $objects$ of a data source, that data source must be saved.\nShould all changes made in this dialog be saved now?";

    Buttons = WB_YES_NO;
