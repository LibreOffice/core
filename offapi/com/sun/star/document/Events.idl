            <dt>OnUnfocus</dt>
            <dt>OnUnload</dt>
            <dt>OnViewCreated</dt>
            <dt>OnViewClosed</dt>
        </dl>
        </p>
        */
    interface com::sun::star::container::XNameReplace;
};


}; }; }; };

#endif

