#ifndef __com_sun_star_awt_Gradient2_idl__
#define __com_sun_star_awt_Gradient2_idl__

#include <com/sun/star/awt/GradientStyle.idl>
#include <com/sun/star/awt/GradientStop.idl>



 module com {  module sun {  module star {  module awt {


/** Describes a gradient between two colors. Many aspects of the
 *  gradient are undefined, like the algorithm and color space to use
 *  to interpolate between the colors and what "intensity" means.
 */
struct Gradient2 : Gradient
{

    /** specifies list of gradient stops characterized by the gradient colors
     *  and their relative positions in the color band.
     */
    sequence< com::sun::star::awt::GradientStop> GradStops;

};


}; }; }; };

