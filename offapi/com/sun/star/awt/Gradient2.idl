#include <com/sun/star/awt/GradientStop.idl>



 module com {  module sun {  module star {  module awt {


/** awt::Gradient supports only two colors Start and End.
  * However a gradient fill can also be characterized by multiple stops.
  * Each Stop is assigned a color and has a relative position in the
  * the fill (color band).
 */
struct Gradient2 : Gradient
{

    /** specifies list of gradient stops
     */
    sequence< com::sun::star::awt::GradientStop> GradientStops;

};


}; }; }; };

