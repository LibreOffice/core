#ifndef __com_sun_star_text_GenericTextField_idl__
#define __com_sun_star_text_GenericTextField_idl__

#include <com/sun/star/text/XTextField.idl>



 module com {  module sun {  module star {  module text { module textfield {


/** A TextField is a TextContent which fades its
    textual representation into the text range to which it is anchored.
    A Generic TextField is added to handle currently unsupported text fields
    in libreoffice. It will store all unsupported fields.

    @since LibreOffice 4.3
 */
published service GenericTextField
{
    service  com::sun::star::text::TextField;

    /** This flag sets string value of current presentation of
        generic text field.
        @since LibreOffice 4.3
    */
    [property] string CurrentPresentation;

    /** This flag stores string value of field value in text field.
        @since LibreOffice 4.3
    */
    [optional, property] string GenericTextFieldText;
};



}; }; }; };

#endif

