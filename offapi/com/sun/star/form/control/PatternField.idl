    @see com::sun::star::awt::UnoControl
    @see com::sun::star::awt::UnoControlModel
*/
published service PatternField
{
    service com::sun::star::awt::UnoControlPatternField;

    interface com::sun::star::form::XBoundControl;
};


}; }; }; }; };

