module com { module sun { module star { module ui {

/** allows to retrieve the user interface configuration manager related to
    an object.

    @since OOo 2.0.0
*/

interface XUIConfigurationManagerSupplier : ::com::sun::star::uno::XInterface
{
    //----------------------------------------------------------------------
