    //---------------------------------------------------------------------
    /**
    */
    const short LISTBOX_VERSION        = 107;

    //---------------------------------------------------------------------
    /**
    */
    const short LISTBOX_TEMPLATE       = 108;


    //---------------------------------------------------------------------
