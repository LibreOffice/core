/* Names and descriptions of the Draw/Impress accessibility views
   ==============================================================
*/

String SID_SD_A11Y_D_DRAWVIEW_N
{
    Text [ de ] = "Zeichnungsansicht" ;
    Text [ en-US ] = "Drawing View";
};

