#include "glob.hrc"
#include "slideshow.hrc"

Menu RID_SLIDESHOW_CONTEXTMENU
{
    ItemList =
    {
        MenuItem
        {
            Identifier = CM_NEXT_SLIDE ;
            Text [ en-US ] = "~Next" ;
        };
        MenuItem
        {
            Identifier = CM_PREV_SLIDE ;
            Text [ en-US ] = "~Previous" ;
           };
        MenuItem
        {
            Identifier = CM_GOTO;
            Text [ en-US ] = "~Go to Slide" ;
            SubMenu = Menu
            {
                ItemList =
                {
                    MenuItem
                    {
                        Identifier = CM_FIRST_SLIDE;
                        Text [ en-US ] = "~First Slide";
                    };
                    MenuItem
                    {
                        Identifier = CM_LAST_SLIDE;
                        Text [ en-US ] = "~Last Slide";
                    };
                    MenuItem
                    {
                        Separator = TRUE;
                    };
                };
            };
        };
        MenuItem
        {
            Separator = TRUE;
        };
        MenuItem
        {
            Identifier = CM_SCREEN;
            Text [ en-US ] = "~Screen" ;

            SubMenu = Menu
            {
                ItemList =
                {
                    MenuItem
                    {
                        Identifier = CM_SCREEN_BLACK;
                        Text [ en-US ] = "~Black";
                    };
                    MenuItem
                    {
                        Identifier = CM_SCREEN_WHITE;
                        Text [ en-US ] = "~White";
                    };
                };
            };
        };
        MenuItem
        {
            Separator = TRUE;
        };
        MenuItem
        {
            Identifier = CM_ENDSHOW ;
            Text [ en-US ] = "~End Show" ;
        };
    };
};
