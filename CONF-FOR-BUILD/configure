$as_echo "found Windows SDK 7 ($WINDOWS_SDK_HOME)" >&6; }
    else
        as_fn_error $? "Found legacy Windows Platform SDK ($WINDOWS_SDK_HOME)" "$LINENO" 5
    fi
    PathFormat "$WINDOWS_SDK_HOME"
    WINDOWS_SDK_HOME="$formatted_path"
