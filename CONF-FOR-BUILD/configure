            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: gmake 3.81 crashes with parallelism > 1, reducing it to 1. upgrade to 3.82 to avoid this." >&5
$as_echo "$as_me: WARNING: gmake 3.81 crashes with parallelism > 1, reducing it to 1. upgrade to 3.82 to avoid this." >&2;}
