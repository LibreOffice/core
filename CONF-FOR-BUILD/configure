if test -d "./translations"; then
    for l in `ls -1 translations/source`; do
        if test ! -d translations/source/$l/helpcontent2; then
            WITH_POOR_HELP_LOCALIZATIONS="$WITH_POOR_HELP_LOCALIZATIONS $l"
        fi
    done
fi



