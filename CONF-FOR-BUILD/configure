        SOLARINC="$SOLARINC -I$JAVA_HOME/include/linux"
        SOLARINC="$SOLARINC -I$JAVA_HOME/include/native_threads/include"
        ;;

    k*bsd*-gnu*)
        case "$host_cpu" in
        i*86)
            JAVA_ARCH="i386"
            JAVA_TOOLKIT="client"
            ;;
        x86_64)
            JAVA_ARCH="amd64"
            JAVA_TOOLKIT="server"
            ;;
