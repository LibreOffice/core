@echo off
iff "%SOLARENV%" == "" then
    echo No environment found, please use 'configure' or 'setsolar'
	goto end
endiff
iff "%PERL%" == "" then
	call perl5 -I%SOLARENV%\bin\modules %SOLARENV%\bin\cwsquery.pl %1&
else
	call %PERL% -I%SOLARENV%\bin\modules %SOLARENV%\bin\cwsquery.pl %1&
endiff
:end
